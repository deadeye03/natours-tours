<%- include('head') %>
    <link rel="stylesheet" href="/css/resetPassword.css">

    <body>
        <section class="reset-password new-pass">
            <div class="reset__box">
                <form class="new-password-form">
                    <input type="password" name="newPass" id="newPass" class="input" placeholder="New password"
                        minlength="8">
                    <input type="password" name="newPassConf" id="newPassConf" class="input"
                        placeholder="Confirm password">
                    <button class="btn btn--reset btn--small">Save Password</button>
                </form>
            </div>
        </section>
        <script src="/js/bundle.js"></script>
    </body>

    </html>